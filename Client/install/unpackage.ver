#!/bin/bash
# Unpackages, installs and removes install files.
# Created for use with remote deployment script

echo ' '
echo 'Unpackaging FRESCO Client [ver]' 

tar zxvf FrescoInstall.tar.gz
cd FrescoClientInstall-[ver]-linux
./install
cd $HOME
rm -r $HOME/FrescoClientInstall-[ver]-linux
rm $HOME/FrescoInstall.tar.gz
rm $HOME/unpackage

